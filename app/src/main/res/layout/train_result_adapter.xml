<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tv_trainResultItemName"
        android:layout_width="wrap_content"
        android:textSize="15dp"
        android:layout_alignParentLeft="true"
        android:layout_margin="8dp"
        android:text="name"
        android:layout_height="wrap_content"></TextView>
    <TextView
        android:id="@+id/tv_trainResultItemResult"
        android:textSize="16dp"
        android:layout_margin="8dp"
        android:text="result"
        android:layout_toRightOf="@+id/tv_trainResultItemName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></TextView>
    <TextView
        android:id="@+id/tv_trainResultItemUnit"
        android:textSize="15dp"
        android:text="unit"
        android:layout_toRightOf="@+id/tv_trainResultItemResult"
        android:layout_width="wrap_content"
        android:layout_margin="8dp"
        android:layout_height="wrap_content"></TextView>
    <TextView
        android:text="reference"
        android:id="@+id/tv_trainResultItemReference"
        android:textSize="15dp"
        android:layout_margin="8dp"
        android:layout_toLeftOf="@+id/tv_trainResultItemJudge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></TextView>
    <TextView
        android:id="@+id/tv_trainResultItemJudge"
        android:textSize="15dp"
        android:text="judge"
        android:layout_alignParentRight="true"
        android:layout_margin="8dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></TextView>

</RelativeLayout>
